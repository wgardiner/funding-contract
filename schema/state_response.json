{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "StateResponse",
  "type": "object",
  "required": [
    "name",
    "proposer_whitelist",
    "voter_whitelist"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "proposal_period_end": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "proposal_period_start": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "proposer_whitelist": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/HumanAddr"
      }
    },
    "voter_whitelist": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/HumanAddr"
      }
    },
    "voting_period_end": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "voting_period_start": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    }
  }
}
